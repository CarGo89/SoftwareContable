@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="">
    <form class="form-horizontal">
        <div class="breadcrumbs-v3">
            <div class="container">
                <h1 class="pull-left">Clientes</h1>
            </div>
        </div>

        <div class="container table-results-container">
            <div class="row text-center">
                <div class="col-md-offset-5 col-md-2">
                    <button type="button" title="Agregar Nuevo Cliente" class="btn btn-block btn-success" data-toggle="modal" data-target="#newApplicationModal">
                        <i class="fa fa-plus-square-o"></i>Agregar
                    </button>
                </div>
            </div>

            <div class="row-fluid voffset4">
                <div class="col-md-12">
                    <table id="applicationResults" class="table table-striped table-bordered table-hover dt-responsive" width="100%">
                        <thead>
                            <tr>
                                <th>RCF</th>
                                <th>Raz&oacute;n Social</th>
                                <th>Tel&eacute;fono</th>
                                <th>Email</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </form>
</div>
